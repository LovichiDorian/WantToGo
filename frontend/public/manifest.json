  "categories": ["travel", "navigation", "lifestyle", "social", "games"],
  "shortcuts": [
    {
      "name": "Add Place",
      "short_name": "Add",
      "description": "Add a new place to visit",
      "url": "/add",
      "icons": [{ "src": "/icons/icon-192x192.png", "sizes": "192x192" }]
    },
    {
      "name": "View Map",
      "short_name": "Map",
      "description": "See all places on the map",
      "url": "/map",
      "icons": [{ "src": "/icons/icon-192x192.png", "sizes": "192x192" }]
    },
    {
      "name": "Leaderboard",
      "short_name": "Rank",
      "description": "Check your ranking",
      "url": "/leaderboard",
      "icons": [{ "src": "/icons/icon-192x192.png", "sizes": "192x192" }]
    },
    {
      "name": "Profile",
      "short_name": "Profile",
      "description": "View your badges and stats",
      "url": "/profile",
      "icons": [{ "src": "/icons/icon-192x192.png", "sizes": "192x192" }]
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/home-dark.png",
      "sizes": "1170x2532",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Home Dashboard with XP and Daily Quests"
    },
    {
      "src": "/screenshots/map-dark.png",
      "sizes": "1170x2532",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Interactive Map with your places"
    },
    {
      "src": "/screenshots/profile-dark.png",
      "sizes": "1170x2532",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Profile with badges and achievements"
    }
  ],
  "related_applications": [],
  "prefer_related_applications": false,
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "share_target": {
    "action": "/add",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "name",
      "text": "notes",
      "url": "url"
    }
  }
}
