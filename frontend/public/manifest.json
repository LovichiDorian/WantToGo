{
  "name": "WantToGo - Gamified Travel Planner",
  "short_name": "WantToGo",
  "description": "Save places, earn XP, unlock badges, compete with friends. The gamified travel app that makes exploring addictive!",
  "start_url": "/",
  "id": "/",
  "display": "standalone",
  "display_override": ["standalone", "minimal-ui"],
  "background_color": "#0f172a",
  "theme_color": "#0ea5e9",
  "orientation": "portrait",
  "scope": "/",
  "lang": "en",
  "dir": "ltr",
